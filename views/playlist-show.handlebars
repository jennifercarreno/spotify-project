<h1>{{playlist.title}}</h1>
{{!-- <p>{{playlist._id.tracks}}</p> --}}
<ol>
    {{#each playlist.tracks}}
        <li>
            <div class="row">

                {{!-- DISPLAYS ALBUM COVER --}}

               <div class="col-sm-1">
                {{#with this.album.images.[2]}}
                <img src={{this.url}}>
                {{/with}}
               </div>

               <div class="col-lg-6">
                {{!-- DISPLAYS TRACK NAME --}}
                   <h3>{{this.name}}</h3>

                {{!-- DISPLAYS ARTIST --}}
                   {{#each this.album.artists}}
                    <p>{{this.name}}</p>
                    {{/each}}
               </div>
        </li>
    {{/each}}
</ol>